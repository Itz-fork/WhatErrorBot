        },
        "MONGODB_URL": {
            "description": "Your mongodb srv url from cloud.mongodb.com",
            "required": true
        }
    },
    "builpacks": [
        {
            "url": "https://github.com/chibat/heroku-buildpack-deno.git"
        }
    ],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "free"
        }
    }
