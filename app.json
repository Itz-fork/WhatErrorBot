{
    "name": "WhatErrorBot",
    "description": "Search for telegram api errors and get information about them without telegram",
    "repository": "https://github.com/Itz-fork/WhatErrorBot",
    "keywords": [
        "telegram api errors",
        "deno",
        "grammy"
    ],
    "website": "https://t.me/NexaBotsUpdates",
    "success_url": "https://t.me/NexaBotsUpdates",
    "env": {
        "BOT_TOKEN": {
            "description": "Your bot api toke from @BotFather",
